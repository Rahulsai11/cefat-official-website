
{
  "framework": "vite",
  "buildCommand": "npm install --force && npm run build",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/lovable-uploads/(.*)",
      "dest": "/lovable-uploads/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
